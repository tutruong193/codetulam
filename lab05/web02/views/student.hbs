<table border ="1">
    <tr>
        <th colspan="4">Student list</th>
    </tr>
    <tr>
        <th>Student name</th>
        <th>Student dob</th>
        <th>Student gender</th>
        <th>Student GPA</th>
        <th>Student Ava</th>
        <th>Delete</th>
    </tr>
    {{#each students}}
    <tr>
        <th>{{name}}</th>
        <th>{{dateFormat dob "DD/MM/YYYY"}}</th>
        <th>{{gender}}</th>
        <th>{{gpa}}</th>
        <th><img src="{{ava}}" width="100" height="100"></th>
        <th><button><a href="/student/delete/{{_id}}">Delete</a></button></th>
    </tr>
    {{/each}}
</table>
<button><a href="/student/drop">delete all</a></button>